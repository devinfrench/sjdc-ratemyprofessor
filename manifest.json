{
  "name": "SJDC RateMyProfessor Ratings",
  "description": "Shows professors' RateMyProfessor ratings in the class listings.",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "*://*/*"
  ],
  "content_scripts": [ {
    "js": [ "content.js" ],
    "matches": [ "https://classes.deltacollege.edu/schedule/*", "http://classes.deltacollege.edu/schedule/*"]
  }],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}